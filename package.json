{
  "name": "card-managment-app",
  "version": "0.1.0",
  "private": true,
  "description": "This is a React application that displays popular images from Unsplash. The application supports endless scrolling, the ability to like/cancel likes, filtering by tear-stained images and viewing image details on a separate page.",
  "main": "index.ts",
  "homepage": "https://notacat1.github.io/Card-Management-App",
  "repository": {
    "type": "git",
    "url": "https://github.com/NotACat1/Card-Management-App"
  },
  "keywords": [
    "Unsplash",
    "React",
    "Redux",
    "SCSS",
    "ESLint",
    "Prettier",
    "Stylelint",
    "HTMLHint",
    "Husky",
    "Bootstrap"
  ],
  "author": "NotACat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NotACat1/Card-Management-App/issues"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react-redux": "^7.1.33",
    "axios": "^1.7.4",
    "bootstrap": "^5.3.3",
    "craco-alias": "^3.0.1",
    "react-bootstrap": "^2.10.4",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "start": "craco start",
    "build": "craco build",
    "test": "jest",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.12",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redux": "^5.0.1",
    "sass": "^1.77.8",
    "stylelint": "^16.8.2",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.5"
  }
}
